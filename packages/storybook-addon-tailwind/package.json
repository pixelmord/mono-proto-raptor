{
  "name": "storybook-addon-tailwind",
  "version": "0.0.0",
  "description": "tailwind addon for storybook, adds dark mode switcher",
  "keywords": [
    "storybook-addons"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/pixelmord/mono-proto-raptor.git"
  },
  "license": "MIT",
  "author": "package-author",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./manager": {
      "require": "./dist/manager.js",
      "import": "./dist/manager.mjs",
      "types": "./dist/manager.d.ts"
    },
    "./preview": {
      "require": "./dist/preview.js",
      "import": "./dist/preview.mjs",
      "types": "./dist/preview.d.ts"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "*.js",
    "*.d.ts"
  ],
  "scripts": {
    "build": "tsup",
    "build-storybook": "storybook build",
    "build:watch": "yarn build --watch",
    "clean": "rimraf ./dist",
    "dev:aux": "concurrently \"yarn build:watch\" \"yarn storybook --quiet\"",
    "prerelease": "zx scripts/prepublish-checks.mjs",
    "release": "pnpm build && auto shipit",
    "storybook": "storybook dev -p 6006",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.0.0-rc.3",
    "@storybook/addon-interactions": "^7.0.0-rc.3",
    "@storybook/addon-links": "^7.0.0-rc.3",
    "@storybook/react": "^7.0.0-rc.3",
    "@storybook/react-vite": "^7.0.0-rc.3",
    "@storybook/testing-library": "^0.0.14-next.1",
    "@types/concurrently": "^6.4.0",
    "@types/dedent": "^0.7.0",
    "@types/node": "^18.15.0",
    "@types/prettier": "^2.7.2",
    "@types/prompts": "^2.4.3",
    "@types/prop-types": "^15.7.5",
    "@types/react": "^18.0.29",
    "@types/react-dom": "^18.0.11",
    "@types/rimraf": "^3.0.2",
    "@vitejs/plugin-react": "^3.1.0",
    "auto": "^10.3.0",
    "boxen": "^5.0.1",
    "concurrently": "^6.2.0",
    "dedent": "^0.7.0",
    "prettier": "^2.3.1",
    "prompts": "^2.4.2",
    "prop-types": "^15.8.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "rimraf": "^3.0.2",
    "storybook": "^7.0.0-rc.3",
    "tsconfig": "workspace:*",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "zx": "^1.14.1"
  },
  "peerDependencies": {
    "@storybook/blocks": "^7.0.0-rc.3",
    "@storybook/components": "^7.0.0-rc.3",
    "@storybook/core-events": "^7.0.0-rc.3",
    "@storybook/manager-api": "^7.0.0-rc.3",
    "@storybook/preview-api": "^7.0.0-rc.3",
    "@storybook/theming": "^7.0.0-rc.3",
    "@storybook/types": "^7.0.0-rc.3",
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "storybook": {
    "displayName": "Addon Kit",
    "supportedFrameworks": [
      "supported-frameworks"
    ],
    "icon": "https://user-images.githubusercontent.com/321738/63501763-88dbf600-c4cc-11e9-96cd-94adadc2fd72.png"
  }
}
